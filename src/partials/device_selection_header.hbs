{{else}}
  <h1 class="heading">{{device.type}} {{headerEnd}}</h1>
{{/ifEq}}
{{#ifEq device.type "TOTP"}}
  <div class="text-block" data-id="textblock">
    <div class="text-block--overflow-wrap">
      Enter the passcode displayed in your app to complete authentication.
    </div>
  </div>
{{else ifEq device.type "Android"}}
  <div class="text-block" data-id="textblock">
    <div class="text-block--overflow-wrap">
      Enter the passcode displayed in your app
      {{#if device.pushEnabled}}
        or
        <a data-mfa-selection="{{device.id}}" data-id="anchor">retry authentication</a>
      {{else}}
        to complete authentication.
      {{/if}}
    </div>
  </div>
{{else ifEq device.type "iPhone"}}
  <div class="text-block" data-id="textblock">
    <div class="text-block--overflow-wrap">
      Enter the passcode displayed in your app
      {{#if device.pushEnabled}}
        or
        <a data-mfa-selection="{{device.id}}" data-id="anchor">retry authentication</a>
      {{else}}
        to complete authentication.
      {{/if}}
    </div>
  </div>
{{else ifEq device.type "Voice"}}
  <div class="text-block" data-id="textblock">
    <div class="text-block--overflow-wrap">
      Enter the passcode you heard to complete authentication.
    </div>
  </div>
{{else ifEq device.type "OATH_TOKEN"}}
  <div class="text-block" data-id="textblock">
    <div class="text-block--overflow-wrap">
      Enter the passcode displayed on your Token.
    </div>
  </div>
{{else}}
  <div class="text-block" data-id="textblock">
    <div class="text-block--overflow-wrap">
      Enter the passcode that you received to complete authentication.
    </div>
  </div>
{{/ifEq}}
