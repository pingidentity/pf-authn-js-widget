            </svg>
          {{else ifEq device.type "Voice"}}
            <svg viewBox="0 0 24 24" role="presentation" style="width: 32px; height: 32px;">
              <path d="M20,15.5C18.8,15.5 17.5,15.3 16.4,14.9C16.3,14.9 16.2,14.9 16.1,14.9C15.8,14.9 15.6,15 15.4,15.2L13.2,17.4C10.4,15.9 8,13.6 6.6,10.8L8.8,8.6C9.1,8.3 9.2,7.9 9,7.6C8.7,6.5 8.5,5.2 8.5,4C8.5,3.5 8,3 7.5,3H4C3.5,3 3,3.5 3,4C3,13.4 10.6,21 20,21C20.5,21 21,20.5 21,20V16.5C21,16 20.5,15.5 20,15.5M5,5H6.5C6.6,5.9 6.8,6.8 7,7.6L5.8,8.8C5.4,7.6 5.1,6.3 5,5M19,19C17.7,18.9 16.4,18.6 15.2,18.2L16.4,17C17.2,17.2 18.1,17.4 19,17.4V19Z"
                    style="fill: rgb(61, 69, 77);"></path>
            </svg>
          {{else}}
            <svg viewBox="0 0 24 24" role="presentation" style="width: 32px; height: 32px">
              <path d="M16,18H7V4H16M11.5,22A1.5,1.5 0 0,1 10,20.5A1.5,1.5 0 0,1 11.5,19A1.5,1.5 0 0,1 13,20.5A1.5,1.5 0 0,1 11.5,22M15.5,1H7.5A2.5,2.5 0 0,0 5,3.5V20.5A2.5,2.5 0 0,0 7.5,23H15.5A2.5,2.5 0 0,0 18,20.5V3.5A2.5,2.5 0 0,0 15.5,1Z"
                    style="fill: rgb(61, 69, 77);"></path>
            </svg>
          {{/ifEq}}
        </div>
      </div>
      <div id="{{device.id}}_device_container" class="tile-button__content-container" data-mfa-selection="{{device.id}}">
        <div class="flex-row flex-row--align-stretch flex-row--justify-start flex-row--flex-direction-column flex-row--column-spacing-sm"
          data-id="flex-row">
          {{#if device.nickname}}
            {{setVariable "deviceDisplayName" device.nickname}}
          {{else}}
            {{#ifEq device.type "Android"}}
              {{setVariable "deviceDisplayName" 'PingOne Authenticator'}}
            {{else ifEq device.type "iPhone"}}
              {{setVariable "deviceDisplayName" "PingOne Authenticator"}}
            {{else ifEq device.type "TOTP"}}
              {{setVariable "deviceDisplayName" "Authenticator App"}}
            {{else ifEq device.type "SMS"}}
              {{setVariable "deviceDisplayName" "TextMessage"}}
            {{else ifEq device.type "Email"}}
              {{setVariable "deviceDisplayName" "Email"}}
            {{else ifEq device.type "SECURITY_KEY"}}
              {{setVariable "deviceDisplayName" "Security  Key"}}
            {{else ifEq device.type "PLATFORM"}}
              {{setVariable "deviceDisplayName" "Biometrics"}}
            {{else ifEq device.type "Voice"}}
              {{setVariable "deviceDisplayName" "Voice"}}
            {{else ifEq device.type "FIDO2"}}
              {{setVariable "deviceDisplayName" "PassKey"}}
            {{else}}
              {{setVariable "deviceDisplayName" device.type}}
            {{/ifEq}}
          {{/if}}
          <div class="tile-button__title">{{@root.deviceDisplayName}}</div>
          {{#ifEq device.type "Android"}}
          <div class="tile-button__content">{{device.name}}</div>
          {{else ifEq device.type "iPhone"}}
          <div class="tile-button__content">{{device.name}}</div>
          {{else if device.target}}
            {{#ifNotEq device.type "SECURITY_KEY"}}
              <div class="tile-button__content">{{device.target}}</div>
            {{/ifNotEq}}
            {{#ifEq device.type "SECURITY_KEY"}}
              <div class="tile-button__content"></div>
            {{/ifEq}}
            {{#ifEq device.type "FIDO2"}}
              <div class="tile-button__content"></div>
            {{/ifEq}}
          {{/ifEq}}
          {{#if device.defaultDevice}}
          <div class="device-selector__item-status"><span class="muted-text">Default</span></div>
          {{/if}}
        </div>
      </div>
      <div id="{{device.id}}_edit_nickname_id" class="saveable-textinput" style="display: none">
        <div class="saveable-textinput__input">
          <input id="nickname_input_for_{{device.id}}" class="text-input"
                 data-id="text-input" placeholder="Nickname" type="text"
                 maxlength="100" value="{{@root.deviceDisplayName}}" data-origin-name="{{@root.deviceDisplayName}}"
                 style="position: inherit; padding-right: 48px">
          <div>
            <button id="save_nickname_button" class="saveable-textinput__save saveable-textinput"
                    data-id="button" type="button" role="button" data-mfa-selection="{{device.id}}"
                    style="border:none; position:inherit"></button>
          </div>
        </div>
      </div>
      {{#if ../manageDevicesAllowed}}
        <div class="device-selector__item-controls" id="kebab-menu-icon-id" data-mfa-selection-kebab-menu-container="{{device.id}}">
          <div class="popover popover--left" data-id="popover-nav-menu">
            <a class="popover__trigger" data-id="popover-nav-menu-trigger">
              <div>
                <button class="button file-input~~button popover-menu__control" data-id="button" style="border:none" type="button">
                  <svg id="kebab-menu-svg-id" viewBox="0 0 24 24" role="presentation" style="width: 26px; height: 26px;">
                    <path
                      d="M12,16A2,2 0 0,1 14,18A2,2 0 0,1 12,20A2,2 0 0,1 10,18A2,2 0 0,1 12,16M12,10A2,2 0 0,1 14,12A2,2 0 0,1 12,14A2,2 0 0,1 10,12A2,2 0 0,1 12,10M12,4A2,2 0 0,1 14,6A2,2 0 0,1 12,8A2,2 0 0,1 10,6A2,2 0 0,1 12,4Z"
                      style="fill: rgb(104, 111, 119);"></path>
                  </svg>
                </button>
              </div>
            </a>
            <div id="device-management-popup-frame-{{device.id}}"
                 class="popper-container popover-display" ;
                 style="display:none; position: absolute; right: 0px;">
              <div class="popup-frame popup-frame--left">
                <div class="popover-menu__dialog" data-id="kebab-menu">
                  {{#if ../userSelectedDefault}}
                    {{#ifEq device.defaultDevice false}}
                      <div id="set-default-device-{{device.id}}" data-mfa-selection-kebab-menu-container="{{device.id}}" data-id="popup-frame" data-parent="popover-nav-menu">
                        <button class="button file-input--button popover-menu__item" data-id="button" type="button">Set As Default</button>
                      </div>
                    {{/ifEq}}
                  {{/if}}
                  <div id="edit-device-{{device.id}}" data-mfa-selection-kebab-menu-container="{{device.id}}" data-id="popup-frame" data-parent="popover-nav-menu">
                    <button class="button file-input--button popover-menu__item" data-id="button" type="button">Edit Name</button>
                  </div>
                  <div id="remove-device-{{device.id}}" data-mfa-selection-kebab-menu-container="{{device.id}}" data-id="popup-frame" data-parent="popover-nav-menu">
                    <button class="button file-input--button popover-menu__item" data-id="button" type="button">Remove</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="saveable-textinput__cancel" id="cancel_edit_name_icon_id" data-mfa-selection="{{device.id}}" style="display: none">
        </div>
      {{/if}}
    </div>
    {{/isValidRpId}}
  {{/if}}
{{/each}}
